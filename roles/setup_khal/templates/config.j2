[calendars]

[[shared_local]]
path = ~/.local/share/calendars/shared/*
type = discover

[[personal_local]]
path = ~/.local/share/calendars/*
type = discover

[locale]
timeformat = %H:%M
dateformat = %Y-%m-%d
longdateformat = %Y-%m-%d
datetimeformat = %Y-%m-%d %H:%M
longdatetimeformat = %Y-%m-%d %H:%M

[default]
default_calendar = shared
